{
  "name": "mashup-design.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "mashup-design.github.io",
  "author": "HaJunRyu",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint:code": "eslint --ext .js,.ts,.tsx",
    "lint:code-fix": "eslint --fix --ext .js,.ts,.tsx",
    "prettier:code-fix": "prettier --write",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "dotenv": "^16.0.2",
    "gatsby": "^4.21.0",
    "gatsby-plugin-emotion": "^7.21.0",
    "gatsby-plugin-image": "^2.21.0",
    "gatsby-plugin-manifest": "^4.21.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sharp": "^4.21.0",
    "gatsby-plugin-sitemap": "^5.21.0",
    "gatsby-source-contentful": "^7.19.0",
    "gatsby-source-filesystem": "^4.21.0",
    "gatsby-transformer-remark": "^5.21.0",
    "gatsby-transformer-sharp": "^4.21.0",
    "lodash-es": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "svg-react-loader": "^0.4.6"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@yarnpkg/sdks": "^2.6.3",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.json": [
      "yarn prettier:code-fix"
    ],
    ".*rc": [
      "yarn prettier:code-fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:code-fix --cache"
    ]
  },
  "packageManager": "yarn@3.2.2"
}
